 {
  "compilerOptions": {
    "target": "es2022",
    "lib": ["es2023"],
    "module":"nodenext",
    "strict": true,
    "esModuleInterop": true, 
    "skipLibCheck": true,
    "moduleResolution": "node16",

    "preserveConstEnums": true,
    "noEmit": true,
    "sourceMap": false,
    "forceConsistentCasingInFileNames": true, 
    "isolatedModules": true, 
    "strictPropertyInitialization": false,
  },
  "exclude": ["node_modules", "**/*.test.ts"],
  "include": ["src"]
}